{
  "name": "gen-pass",
  "license": "MIT",
  "version": "1.1.5",
  "main": "server.js",
  "engines": {
    "node": "8.1.4",
    "npm": "5.0.3"
  },
  "scripts": {
    "precommit": "lint-staged",
    "dev": "node server.js",
    "build": "yarn run precommit && next build",
    "build:deploy": "yarn run export",
    "export": "npm run build && next export -o docs",
    "heroku-postbuild": "npm run build",
    "serve": "yarn run build && NODE_ENV=development node server.js",
    "start": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "fastify": "^0.29.2",
    "fela": "^6.1.0",
    "fela-dom": "^7.0.0",
    "fela-preset-web": "^8.0.0",
    "html2canvas": "^1.0.0-alpha.9",
    "next": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-fela": "^6.1.1",
    "react-qr-svg": "^2.1.0"
  },
  "devDependencies": {
    "babel-plugin-transform-define": "^1.3.0",
    "color-convert": "^1.9.1",
    "has-flag": "^3.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.10.2",
    "setprototypeof": "^1.1.0"
  },
  "lint-staged": {
    "*.js": [
      "node_modules/.bin/prettier --single-quote --no-semi --write",
      "git add -f"
    ]
  }
}
