{
  "name": "gen-pass",
  "license": "MIT",
  "version": "1.1.7",
  "main": "server.js",
  "engines": {
    "node": ">=8.1.4",
    "npm": ">=5.0.3"
  },
  "scripts": {
    "precommit": "lint-staged",
    "dev": "node server.js",
    "build": "yarn precommit && next build",
    "build:deploy": "yarn run export",
    "export": "yarn build && next export -o docs && cpy ./config/CNAME ./docs",
    "heroku-postbuild": "yarn build",
    "serve": "yarn build && NODE_ENV=development node server.js",
    "start": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "fastify": "^2.11.0",
    "fela": "^11.0.2",
    "fela-dom": "^11.0.2",
    "fela-preset-web": "^11.0.2",
    "html2canvas": "^1.0.0-alpha.9",
    "next": "^9.3.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-fela": "^11.0.2",
    "react-qr-svg": "^2.2.1"
  },
  "devDependencies": {
    "babel-plugin-transform-define": "^2.0.0",
    "color-convert": "^2.0.1",
    "cpy-cli": "^3.0.0",
    "has-flag": "^4.0.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "setprototypeof": "^1.2.0"
  },
  "lint-staged": {
    "*.js": [
      "node_modules/.bin/prettier --single-quote --no-semi --write",
      "git add -f"
    ]
  }
}
